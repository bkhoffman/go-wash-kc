{
    "name": "GoWashKC",
    "version": "1.0.0",
    "description": "TBD",
    "author": "TBD",
    "license": "MIT",
    "main": "server.js",
    "engines": {
        "node": "12.x"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/TBD"
    },
    "homepage": "https://go-wash-kc.herokuapp.com/",
    "scripts": {
        "start": "if-env NODE_ENV=production && yarn start:prod || yarn start:dev",
        "start:prod": "node server.js",
        "start:dev": "yarn kill && concurrently \"nodemon\" \"yarn client\"",
        "server": "yarn kill && nodemon",
        "client": "cd client && yarn start",
        "install": "cd client && yarn install",
        "build": "cd client && yarn build",
        "heroku-postbuild": "yarn build",
        "kill": "kill-port 3001",
        "lint": "eslint --quiet .",
        "lint:fix": "eslint --fix .",
        "tests": "yarn kill && NODE_ENV=test nyc --reporter=html --reporter=text mocha tests/**/*.spec.js --exit",
        "test:coverage": "nyc report --reporter=text-lcov | coveralls"
    },
    "nodemonConfig": {
        "events": {
            "restart": "yarn kill",
            "crash": "yarn kill"
        },
        "delay": "3000",
        "ignore": [
            "client/*"
        ]
    },
    "nyc": {
        "exclude": [
            "client"
        ]
    },
    "devDependencies": {
        "chai": "^4.2.0",
        "chai-http": "^4.3.0",
        "concurrently": "^4.1.1",
        "coveralls": "^3.0.4",
        "eslint": "^5.16.0",
        "eslint-config-airbnb-base": "^13.1.0",
        "eslint-plugin-import": "^2.17.3",
        "kill-port": "^1.5.1",
        "mocha": "^6.1.4",
        "mocha-lcov-reporter": "^1.3.0",
        "nodemon": "^1.19.1",
        "nyc": "^14.1.1",
        "sequelize-cli": "^5.5.0",
        "supertest": "^4.0.2"
    },
    "dependencies": {
        "@material-ui/core": "^4.2.0",
        "@material-ui/styles": "^4.2.1",
        "axios": "^0.21.1",
        "bcrypt": "^3.0.6",
        "bcrypt-nodejs": "0.0.3",
        "body": "^5.1.0",
        "cookie-parser": "^1.4.4",
        "cors": "^2.8.5",
        "date-fns": "^1.30.1",
        "express": "^4.17.1",
        "express-session": "^1.16.2",
        "helmet": "^3.18.0",
        "if-env": "^1.0.4",
        "mysql": "^2.17.1",
        "mysql2": "^1.6.5",
        "passport": "^0.4.0",
        "passport-local": "^1.0.0",
        "passport-session": "^1.0.2",
        "proptypes": "^1.1.0",
        "react-datepicker": "^2.8.0",
        "react-redux": "^7.1.0",
        "redux": "^4.0.4",
        "sequelize": "^5.10.1"
    }
}
